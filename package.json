{
  "name": "conteg-react-template-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pre-commit": "yarn lint:fix && yarn type-check",
    "lint": "yarn eslint src --ext .tsx --ext .ts",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-graphql-types": "graphql-codegen --config codegen.yml",
    "cy:run": "npx cross-env WAIT_ON_TIMEOUT=200000 cypress run",
    "test": "start-server-and-test dev http://localhost:5173 cy:run"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@tanstack/react-query": "^4.14.6",
    "graphql": "^16.6.0",
    "i18next": "^22.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.3",
    "react-i18next": "^12.0.0",
    "react-router-dom": "6.4.3",
    "styled-components": "^5.3.6",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-query": "^4.0.5",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@tanstack/react-query-devtools": "^4.19.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^9.1.0",
    "cypress": "^11.0.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.2",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.15.2",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "packageManager": "yarn@3.3.0"
}
